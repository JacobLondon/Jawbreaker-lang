{
    "name": "jb",
    "displayName": "Jawbreaker",
    "description": "Jawbreaker-lang support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
